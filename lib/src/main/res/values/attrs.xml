<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="UrlSpannableTextView">
        <attr name="spannableText" format="string"/>

        <attr name="normalTextColor" format="color"/>
        <attr name="urlValueColor" format="color"/>

        <attr name="highLightColor" format="color"/>
        <attr name="enableHighLightOfNormalText" format="boolean"/>
        <attr name="enableHighLightOfUrlValue" format="boolean"/>

        <attr name="enableUnderLineOfNormalText" format="boolean"/>
        <attr name="enableUnderLineOfUrlValue" format="boolean"/>
    </declare-styleable>
</resources>